{
	"name": "owl",
	"version": "1.0.0",
	"description": "React starter / educational / inspirational application",
	"keywords": [
		"react",
		"vite",
		"owl",
		"seed",
		"template",
		"boilerplate"
	],
	"author": "AGDevX",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=18",
		"yarn": ">=1"
	},
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"createLocalSsl": "node ./localSsl/create-local-ssl.js win",
		"lint": "eslint . -c eslint.commit.js",
		"lint-local": "eslint . -c eslint.local.js",
		"buildServiceWorker": "rollup -c ./service-worker/rollup.sw.js",
		"build": "yarn buildServiceWorker && vite build --emptyOutDir",
		"watch": "yarn buildServiceWorker && vite serve --c vite.config.js",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/preset-env": "^7.22.20",
		"@eslint/js": "^8.45.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^25.0.5",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-replace": "^5.0.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@vitejs/plugin-react": "^4.0.3",
		"eslint": "^8.0.1",
		"eslint-plugin-react": "^7.33.0",
		"globals": "^13.20.0",
		"hostile": "^1.3.3",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"vite": "^4.4.6",
		"vite-plugin-html": "^3.2.0",
		"workbox-recipes": "^7.0.0",
		"workbox-routing": "^7.0.0",
		"workbox-strategies": "^7.0.0"
	},
	"dependencies": {
		"@auth0/auth0-react": "^2.0.0",
		"@reduxjs/toolkit": "^1.9.1",
		"localforage": "^1.10.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.1",
		"react-router-dom": "^6.14.2"
	}
}
